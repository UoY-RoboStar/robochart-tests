interface i {
	event ins
}
interface o {
	event outs
}

module anncont2 {
	anncontroller annctrl2 {
			params {
				input context {
					uses i
				}
				output context {
					uses o
				}
				layerstructure <1>
				weights <<<0.0>>>
				biases <<0.0>>
				activationfunction relu
				
			}
	}
	
	robotic platform R0 {
		uses i uses o
	}
	
	connection R0 on outs to annctrl2 on ins ( _async )
	connection annctrl2 on outs to R0 on ins ( _async )
	
}

