interface input {
	event ins
}
interface output {
	event outs
}

module annfail7 {
	anncontroller annfail7 {
			params {
				inputcontext {
					uses input
				}
				outputcontext {
					uses output
				}
				layerstructure <1>
				weights <<<0.0>>>
				biases <<0.0>>
				activationfunction relu
				
			}
	}
	
	robotic platform R0 {
		uses input uses output
	}
	
	//connection R0 on outs to annfail7 on ins ( _async )
	connection annfail7 on outs to R0 on ins ( _async )
	
}