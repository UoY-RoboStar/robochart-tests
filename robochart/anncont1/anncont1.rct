interface input {
	event ins
}
interface output {
	event outs
}
module anncont1 {
	anncontroller annctrl1 {
			params {
				inputcontext {
					uses input
				}
				outputcontext {
					uses output
				}
				layerstructure <1>
				weights <<<0.0>>>
				biases <<0.0>>
				activationfunction relu
				
			}
	}
	
	robotic platform R0 {
		uses input uses output
	}
	
	connection annctrl1 on outs to R0 on ins ( _async )
	connection R0 on outs to annctrl1 on ins ( _async )
	
}